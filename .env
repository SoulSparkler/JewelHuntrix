# Database - Using Supabase pooler connection as specified
DATABASE_URL=postgresql://postgres:password@xxxx.pooler.supabase.com:6543/postgres?sslmode=require

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Vinted Session Management
VINTED_SESSION_FILE=./data/vinted-session.json
VINTED_REGION=NL

# Legacy session cookie support
VINTED_SESSION_COOKIE=your_session_cookie
VINTED_AUTH_TOKEN=your_auth_token

# Enhanced Scheduler (Anti-blocking)
SCAN_MIN_INTERVAL_MINUTES=70
SCAN_MAX_INTERVAL_MINUTES=150
SCAN_FREQUENCY_HOURS=2

# Telegram
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id